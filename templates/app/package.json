{
  "name": "gotron-browser-window",
  "version": "0.1.0",
  "main": "main.js",
  "scripts":{
    "start": "electron .",
    "package": "electron-packager . 'wins' --platform=darwin --arch=x64  --out=./dist  --app-version=1.0.0 --ignore=\"(dist|docs|.gitignore|LICENSE|README.md|webpack.config*)\"",
    "packageDarwin": "electron-packager . 'wins' --platform=darwin --arch=x64  --out=./dist --asar --app-version=1.0.0 --ignore=\"(dist|src|docs|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\"",
    "packageDarwin2": "electron-packager . 'wins' --platform=darwin --arch=x64  --out=./dist --asar --app-version=1.0.0",
    "packageWin": "electron-packager . 'wins' --platform=win32 --arch=x64  --out=./dist --asar --app-version=1.0.0 --ignore=\"(dist|src|docs|.gitignore|LICENSE|README.md|webpack.config.js|node_modules)\"",
    "packageWin2": "electron-packager . 'wins' --platform=win32 --arch=x64 --out=./dist --asar --app-version=1.0.0"
  },
  "dependencies": {
    "electron-connect": "^0.6.3",
    "websocket": "latest"
  },
  "devDependencies": {
    "electron": "^2.0.15",
    "electron-packager": "^13.1.0"
  }
}
